{
    "type": "AdaptiveCard",
    "body": [
        {
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": "stretch",
                    "items": [
                        {
                            "type": "TextBlock",
                            "size": "Medium",
                            "weight": "Default",
                            "text": "${CompanyName}"
                        },
                        {
                            "type": "TextBlock",
                            "text": "${Symbol}",
                            "wrap": true,
                            "weight": "Lighter"
                        },
                        {
                            "type": "TextBlock",
                            "text": "{{DATE(${lastRefresh}, SHORT)}} {{TIME(${lastRefresh})}}",
                            "wrap": true
                        },
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "items": [
                                        {
                                            "type": "Image",
                                            "width": "20px",
                                            "height": "20px",
                                            "url": "${if(Price > OpeningPrice,'data:image/png;base64,','data:image/png;base64,')}"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "stretch",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "${formatNumber(Price, 2)} ${Currency}",
                                            "wrap": true,
                                            "size": "ExtraLarge"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "High",
                                            "wrap": true,
                                            "weight": "Bolder"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "Low",
                                            "wrap": true,
                                            "weight": "Bolder"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "${formatNumber(HighestPrice, 2)}",
                                            "wrap": true,
                                            "horizontalAlignment": "Right"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "${formatNumber(LowestPrice, 2)}",
                                            "wrap": true,
                                            "horizontalAlignment": "Left"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ],
            "$data": "${$root.value}"
        }
    ],
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5"
}